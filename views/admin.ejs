                        <i class="fa fa-person icon"></i>
                        <input class="input-field" type="text" name="firstName" class="form-control"
                          placeholder="enter first name" onblur="return validateAddUser();"
                          onkeyup="return validateAddUser();" required value="<%=user[i].firstName%>" autofocus />
                      </div>
                      <div class="input-container">
                        <i class="fa fa-person icon"></i>
                        <input class="input-field" type="text" name="lastName" class="form-control"
                          placeholder="enter last name" onblur="return validateAddUser();"
                          onkeyup="return validateAddUser();" required value="<%=user[i].lastName%>" autofocus />
                      </div>
                      <div class="input-container">
                        <i class="fa fa-envelope icon"></i>
                        <input class="input-field" type="email" name="email" class="form-control"
                          placeholder="enter email address" onblur="return validateAddUser();"
                          onkeyup="return validateAddUser();" required value="<%=user[i].email%>" autofocus />
